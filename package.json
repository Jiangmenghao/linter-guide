  "license": "ISC",
  "bugs": {
    "url": "https://github.com/haixiangyan/learn-prettier-linter/issues"
  },
  "homepage": "https://github.com/haixiangyan/learn-prettier-linter#readme",
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "postcss-less": "^5.0.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-less": "^1.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-less": "^1.0.1",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
